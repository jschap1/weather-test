generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(cuid())
  email          String   @unique
  username       String   @unique
  hashedPassword String
  profileImage   String?
  createdAt      DateTime @default(now())
}
